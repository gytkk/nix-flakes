{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "gemini": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "github:aliargun/mcp-server-gemini"
      ],
      "env": {
        "GEMINI_API_KEY": ""
      }
    }
  }
}
